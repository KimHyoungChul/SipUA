<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:fillViewport="true">

	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:orientation="vertical">

		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="50.0dip"
			android:layout_gravity="top"
			android:background="@color/whole_bg"
			android:gravity="center"
			android:orientation="horizontal">

			<ImageButton
				android:id="@id/back_button"
				android:layout_width="66.0dip"
				android:layout_height="36.0dip"
				android:layout_marginLeft="3.0dip"
				android:visibility="invisible"/>

			<TextView
				android:id="@id/search_edittext"
				android:layout_width="0.0dip"
				android:layout_height="wrap_content"
				android:layout_marginLeft="8.0dip"
				android:layout_marginRight="8.0dip"
				android:layout_weight="1.0"
				android:gravity="center"
				android:text="@string/settings"
				android:textColor="@color/font_color3"
				android:textSize="20.0dip"/>

			<ImageButton
				android:id="@id/set_button"
				android:layout_width="66.0dip"
				android:layout_height="36.0dip"
				android:visibility="invisible"/>
		</LinearLayout>

		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="0.0dip"
			android:layout_weight="1.0"
			android:background="#ffffffff"
			android:orientation="vertical"
			android:paddingBottom="10.0dip"
			android:paddingLeft="5.0dip"
			android:paddingRight="5.0dip">

			<LinearLayout
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:background="#ffffffff"
				android:orientation="vertical">

				<LinearLayout
					android:id="@id/autorun"
					android:layout_width="fill_parent"
					android:layout_height="55.0dip"
					android:background="@android:drawable/list_selector_background"
					android:paddingBottom="10.0dip"
					android:paddingTop="10.0dip">

					<TextView
						android:id="@id/txt1"
						android:layout_width="0.0dip"
						android:layout_height="wrap_content"
						android:layout_gravity="center_vertical"
						android:layout_marginLeft="10.0dip"
						android:layout_marginTop="3.0dip"
						android:layout_weight="1.0"
						android:gravity="center_vertical"
						android:text="@string/auto_start_title"
						android:textAppearance="?android:textAppearanceMedium"
						android:textColor="#ff000000"/>

					<com.zed3.utils.SwitchButton
						android:id="@id/imgviewbtn"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_gravity="center_vertical"
						android:gravity="center_vertical"/>
				</LinearLayout>

				<LinearLayout
					android:id="@id/autorun_line0"
					android:layout_width="fill_parent"
					android:layout_height="1.0dip"
					android:background="#ffeeeeee"
					android:orientation="horizontal"/>

				<LinearLayout
					android:id="@id/acceptauto"
					android:layout_width="fill_parent"
					android:layout_height="55.0dip"
					android:background="@android:drawable/list_selector_background"
					android:paddingBottom="10.0dip"
					android:paddingTop="10.0dip">

					<TextView
						android:id="@id/txt2"
						android:layout_width="0.0dip"
						android:layout_height="wrap_content"
						android:layout_gravity="center_vertical"
						android:layout_marginLeft="10.0dip"
						android:layout_marginTop="3.0dip"
						android:layout_weight="1.0"
						android:gravity="center_vertical"
						android:text="@string/meeting_model"
						android:textAppearance="?android:textAppearanceMedium"
						android:textColor="#ff000000"/>

					<com.zed3.utils.SwitchButton
						android:id="@id/imgviewbt"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_gravity="center_vertical"
						android:gravity="center_vertical"/>
				</LinearLayout>

				<LinearLayout
					android:id="@id/autorun_line"
					android:layout_width="fill_parent"
					android:layout_height="1.0dip"
					android:background="#ffeeeeee"
					android:orientation="horizontal"/>

				<LinearLayout
					android:id="@id/line_change_pwd"
					android:layout_width="fill_parent"
					android:layout_height="55.0dip"
					android:background="@android:drawable/list_selector_background"
					android:paddingBottom="10.0dip"
					android:paddingTop="10.0dip">

					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_gravity="center_vertical"
						android:layout_marginLeft="10.0dip"
						android:layout_weight="1.0"
						android:text="@string/setting_change_pwd"
						android:textAppearance="?android:textAppearanceMedium"
						android:textColor="#ff000000"/>

					<ImageView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_alignParentRight="true"
						android:layout_gravity="center_vertical"
						android:paddingRight="20.0dip"
						android:src="@drawable/arrow"/>
				</LinearLayout>

				<LinearLayout
					android:layout_width="fill_parent"
					android:layout_height="1.0dip"
					android:background="#ffeeeeee"
					android:orientation="horizontal"/>

				<LinearLayout
					android:id="@id/linesuper"
					android:layout_width="fill_parent"
					android:layout_height="55.0dip"
					android:background="@android:drawable/list_selector_background"
					android:paddingBottom="10.0dip"
					android:paddingTop="10.0dip">

					<TextView
						android:layout_width="0.0dip"
						android:layout_height="wrap_content"
						android:layout_gravity="center_vertical"
						android:layout_marginLeft="10.0dip"
						android:layout_weight="1.0"
						android:text="@string/advanced_option"
						android:textAppearance="?android:textAppearanceMedium"
						android:textColor="#ff000000"/>

					<ImageView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_alignParentRight="true"
						android:layout_gravity="center_vertical"
						android:paddingRight="20.0dip"
						android:src="@drawable/arrow"/>
				</LinearLayout>

				<LinearLayout
					android:id="@id/linesuper_line"
					android:layout_width="fill_parent"
					android:layout_height="1.0dip"
					android:background="#ffeeeeee"
					android:orientation="horizontal"/>

				<LinearLayout
					android:id="@id/lineflow"
					android:layout_width="fill_parent"
					android:layout_height="55.0dip"
					android:background="@android:drawable/list_selector_background"
					android:paddingBottom="10.0dip"
					android:paddingTop="10.0dip">

					<TextView
						android:layout_width="0.0dip"
						android:layout_height="wrap_content"
						android:layout_gravity="center_vertical"
						android:layout_marginLeft="10.0dip"
						android:layout_weight="1.0"
						android:text="@string/setting_monitoring"
						android:textAppearance="?android:textAppearanceMedium"
						android:textColor="#ff000000"/>

					<ImageView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_alignParentRight="true"
						android:layout_gravity="center_vertical"
						android:paddingRight="20.0dip"
						android:src="@drawable/arrow"/>
				</LinearLayout>

				<LinearLayout
					android:id="@id/lineflow_underline"
					android:layout_width="fill_parent"
					android:layout_height="1.0dip"
					android:background="#ffeeeeee"
					android:orientation="horizontal"/>

				<LinearLayout
					android:id="@id/lineupdate"
					android:layout_width="fill_parent"
					android:layout_height="55.0dip"
					android:background="@android:drawable/list_selector_background"
					android:paddingBottom="10.0dip"
					android:paddingTop="10.0dip">

					<TextView
						android:layout_width="0.0dip"
						android:layout_height="wrap_content"
						android:layout_gravity="center_vertical"
						android:layout_marginLeft="10.0dip"
						android:layout_weight="1.0"
						android:text="@string/setting_update"
						android:textAppearance="?android:textAppearanceMedium"
						android:textColor="#ff000000"/>

					<ImageView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_alignParentRight="true"
						android:layout_gravity="center_vertical"
						android:paddingRight="20.0dip"
						android:src="@drawable/arrow"/>
				</LinearLayout>

				<LinearLayout
					android:id="@id/linedate"
					android:layout_width="fill_parent"
					android:layout_height="1.0dip"
					android:background="#ffeeeeee"
					tools:ignore="Orientation"/>

				<LinearLayout
					android:id="@id/lineinformation"
					android:layout_width="fill_parent"
					android:layout_height="55.0dip"
					android:background="@android:drawable/list_selector_background"
					android:paddingBottom="10.0dip"
					android:paddingTop="10.0dip">

					<TextView
						android:layout_width="0.0dip"
						android:layout_height="wrap_content"
						android:layout_gravity="center_vertical"
						android:layout_marginLeft="10.0dip"
						android:layout_weight="1.0"
						android:text="@string/personal_information"
						android:textAppearance="?android:textAppearanceMedium"
						android:textColor="#ff000000"/>

					<ImageView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_alignParentRight="true"
						android:layout_gravity="center_vertical"
						android:paddingRight="20.0dip"
						android:src="@drawable/arrow"/>
				</LinearLayout>

				<LinearLayout
					android:layout_width="fill_parent"
					android:layout_height="1.0dip"
					android:background="#ffeeeeee"/>

				<LinearLayout
					android:id="@id/lineabout"
					android:layout_width="fill_parent"
					android:layout_height="55.0dip"
					android:background="@android:drawable/list_selector_background"
					android:paddingBottom="10.0dip"
					android:paddingTop="10.0dip">

					<TextView
						android:layout_width="0.0dip"
						android:layout_height="wrap_content"
						android:layout_gravity="center_vertical"
						android:layout_marginLeft="10.0dip"
						android:layout_weight="1.0"
						android:text="@string/setting_about"
						android:textAppearance="?android:textAppearanceMedium"
						android:textColor="#ff000000"/>

					<ImageView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_alignParentRight="true"
						android:layout_gravity="center_vertical"
						android:paddingRight="20.0dip"
						android:src="@drawable/arrow"/>
				</LinearLayout>

				<LinearLayout
					android:layout_width="fill_parent"
					android:layout_height="1.0dip"
					android:background="#ffeeeeee"/>
			</LinearLayout>

			<Button
				android:id="@id/loginout"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="15.0dip"
				android:background="@drawable/loginoutstyle"
				android:paddingBottom="10.0dip"
				android:paddingLeft="5.0dip"
				android:paddingRight="5.0dip"
				android:paddingTop="10.0dip"
				android:text="@string/setting_out"
				android:textColor="@color/tab_wihte"
				android:textSize="18.0sp"/>
		</LinearLayout>
	</LinearLayout>
</ScrollView>
